platform :ios, '12.0'

source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'

use_frameworks!

target 'Eduoep' do

    pod 'SDWebImage'
    pod 'MJRefresh'
    
    pod 'XCGLogger', '~> 7.0.1'

    # Rx
    pod 'RxSwift'
    pod 'RxCocoa'

    #网络
    pod 'HandyJSON' ,'5.0.1'
    pod 'Alamofire', '~> 5.2'
    pod 'SwiftyJSON', '~> 4.0'
    # UI
    pod 'SnapKit', '5.0.0'
    pod 'IQKeyboardManager', '6.5.5'
    

    # 储存
    pod 'YapDatabase', '4.0.0'
    pod 'KeychainAccess'

    #登录
    pod 'GoogleSignIn' , '5.0.2'
    pod 'FBSDKLoginKit', '7.0.0'
    pod 'WechatOpenSDK', '1.8.7.1'

    # 日志
    pod 'Bugly'
    pod 'XCGLogger', '~> 7.0.1'

end
